{
  "icon": true,
  "ref": true,
  "typescript": true,
  "svgProps": {
    "width": "1.5em",
    "height": "1.5em",
    "color": "currentColor"
  },
  "jsx": {
    "babelConfig": {
      "plugins": [
        [
          "@svgr/babel-plugin-remove-jsx-attribute",
          {
            "elements": [
              "path"
            ],
            "attributes": [
              "strokeWidth"
            ]
          },
          "remove-stroke-width"
        ]
      ]
    }
  }
}
