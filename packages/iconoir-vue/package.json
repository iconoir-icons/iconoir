{
  "name": "@iconoir/vue",
  "version": "7.11.0",
  "description": "Vue library for Iconoir, the biggest open source icon library with tons of free icons.",
  "license": "MIT",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/iconoir"
  },
  "homepage": "https://iconoir.com",
  "repository": {
    "type": "git",
