  "module": "./dist/esm/index.mjs",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "peerDependencies": {
    "vue": "3"
  },
  "devDependencies": {
    "@vue/tsconfig": "^0.7.0"
